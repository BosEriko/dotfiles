[user]
  email = bos.eriko@gmail.com
  name = BosEriko
[credential]
  helper = cache --timeout=36000
[http]
  postBuffer = 1048576000
[core]
  compression = 0
[alias]
  a       = add
  b       = branch
  c       = commit
  co      = checkout
  d       = diff
  dt      = difftool
  graph   = log --graph --decorate --pretty=oneline --abbrev-commit --all
  h       = "!sh -c 'git-help'"
  help    = "!sh -c 'git-help'"
  lg      = log -p
  lol     = log --decorate --pretty=oneline --abbrev-commit
  ls      = ls-files
  pl      = pull
  ps      = push
  r       = "!sh -c 'git-rework'"
  re      = reset
  rework  = "!sh -c 'git-rework'"
  rl      = tag -n1
  s       = status
  st      = stash
[color]
  ui = auto
[hub]
  protocol = ssh
