{
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "l"],
      "commands": ["editor.action.toggleRenderWhitespace"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["editor.action.toggleWordWrap"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["gitlens.toggleFileBlame"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["extension.dash.specific"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.extensions.action.updateAllExtensions"]
    }
  ],
  "breadcrumbs.enabled": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorBlinking": "smooth",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": true,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "all",
  "editor.rulers": [ 100 ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 4,
  "editor.wordWrap": "off",
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": false,
  "files.associations": { "*.hamlc": "haml" },
  "files.autoSave": "off",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.autofetchPeriod": 1000,
  "git.enabled": true,
  "search.location": "panel",
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "DroidSansMono Nerd Font",
  "terminal.integrated.shell.osx": "zsh",
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "black",
  "vim.easymotionMarkerFontWeight": "normal",
  "vim.easymotionMarkerForegroundColorOneChar": "white",
  "vim.easymotionMarkerForegroundColorTwoChar": "red",
  "vim.easymotionMarkerHeight": 20,
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.easymotionMarkerYOffset": 0,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Material Theme",
  "workbench.editor.enablePreview": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.showTabs": true,
  "workbench.iconTheme": "simple-icons",
  "workbench.list.keyboardNavigation": "simple",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": true,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "zenMode.hideActivityBar": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false
}
